# Battery
genfscon sysfs /devices/platform/soc/soc:maxim_ds28e16/power_supply/batt_verify    u:object_r:vendor_sysfs_battery_supply:s0

# Proximity
genfscon sysfs /bus/iio/devices                                                    u:object_r:sysfs_iio_devices:s0
genfscon sysfs /devices/platform/us_prox.0/iio:device2                             u:object_r:sysfs_iio_devices:s0

genfscon sysfs /devices/platform/soc/c440000.qcom,spmi/spmi-0/spmi0-04/c440000.qcom,spmi:qcom,pm6150l@4:vadc@3100/iio:device1
u:object_r:sysfs_iio_devices:s0
