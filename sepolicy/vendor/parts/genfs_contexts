# XiaomiParts (Flashlight Brightness)
genfscon sysfs /class/leds/led:torch_0/max_brightness u:object_r:sysfs_leds:s0

# XiaomiParts (FPS Info)
genfscon sysfs /sys/devices/platform/soc/ae00000.qcom,mdss_mdp/drm/card0/sde-crtc-0/measured_fps  u:object_r:vendor_sysfs_graphics:s0

# XiaomiParts (Haptic)
genfscon sysfs /devices/platform/soc/a8c000.i2c/i2c-2/2-005a/ulevel                  u:object_r:sysfs_vibrator:s0

# XiaomiParts (KProfiles)
genfscon sysfs /module/kprofiles/parameters/auto_kprofiles    u:object_r:vendor_sysfs_kprofiles:s0
genfscon sysfs /module/kprofiles/parameters/kp_mode           u:object_r:vendor_sysfs_kprofiles:s0

# XiaomiParts (Sound-Control nodes)
genfscon sysfs /kernel/sound_control/headphone_gain                u:object_r:sysfs_gain:s0
genfscon sysfs /kernel/sound_control/mic_gain                      u:object_r:sysfs_gain:s0
genfscon sysfs /kernel/sound_control/speaker_gain                  u:object_r:sysfs_gain:s0
